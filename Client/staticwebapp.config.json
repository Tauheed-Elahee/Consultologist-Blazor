{
	"routes": [
		{
			"route": "/admin/*",
			"allowedRoles": ["admin"]
		},
		{
			"route": "/clinician/*",
			"allowedRoles": ["clinician", "admin"]
		},
		{
			"route": "/api/*",
			"allowedRoles": ["authenticated"]
		}
	],
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": [
			"/api/*",
			"/admin/*",
			"/clinician/*",
			"/*.css",
			"/*.js",
			"/*.png",
			"/*.jpg",
			"/*.svg",
			"/favicon.ico"
		]
	},
	"mimeTypes": {
		".json": "application/json",
		".js": "application/javascript",
		".css": "text/css",
		".wasm": "application/wasm"
	}
}
