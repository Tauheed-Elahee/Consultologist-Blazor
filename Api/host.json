{
	"version": "2.0",
	"logging": {
		"logLevel": {
			"default": "Debug",
			"Host.Results": "Information",
			"Function": "Debug",
			"BlazorApp.Api": "Debug",
			"Microsoft.Graph": "Debug",
			"Azure.Identity": "Debug"
		},
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true,
				"excludedTypes": "Request"
			},
			"enableLiveMetricsFilters": true
		}
	}
}
